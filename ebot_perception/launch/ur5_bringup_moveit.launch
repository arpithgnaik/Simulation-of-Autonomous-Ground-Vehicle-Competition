<?xml version="1.0"?>
<launch>
	<include file="$(find ebot_description)/launch/task4.launch" />
	<node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher">
		<param name="publish_frequency" type="double" value="50.0" />
		<param name="tf_prefix" type="string" value="" />
	</node>
	<node pkg="tf" type="static_transform_publisher" name="map_world_broadcaster" args="0 0 0 0 0 0 odom ebot_base 5" />
	<rosparam file="$(find ebot_perception)/config/joint_state_controller.yaml" command="load" />
	<node name="joint_state_controller_spawner" pkg="controller_manager" type="controller_manager" args="spawn joint_state_controller" respawn="false" output="screen" />
	<rosparam file="$(find ebot_perception)/config/trajectory_control.yaml" command="load" />
	<node name="arm_controller_spawner" pkg="controller_manager" type="controller_manager" args="spawn arm_controller gripper_controller" respawn="false" output="screen" />
	<include file="$(find ebot_perception)/launch/planning_context.launch">
		<arg name="load_robot_description" value="true" />
	</include>
	<node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher">
		<param name="/use_gui" value="false" />
		<rosparam param="/source_list">[/joint_states]</rosparam>
	</node>
	<include file="$(find ebot_perception)/launch/move_group.launch">
		<arg name="publish_monitored_planning_scene" value="true" />
	</include>
	<arg name="use_rviz" default="true" />
	<include file="$(find ebot_perception)/launch/moveit_rviz.launch">
		<arg name="rviz_config" value="$(find ebot_perception)/launch/moveit.rviz" />
		<arg name="debug" value="false" />
	</include>
	<include file="$(find ebot_perception)/launch/sensor_manager.launch.xml"/>
	<include file="$(find ebot_perception)/launch/load_octomap.launch"/>
	<include file="$(find ebot_perception)/launch/start_find_object_3d.launch"/>
</launch>
